    "keywords": [ "symfony", "form", "order" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/form": "^3.0|^4.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^7.0",
        "symfony/phpunit-bridge": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": { "Ivory\\OrderedForm\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Ivory\\Tests\\OrderedForm\\": "tests/" }
    }
}
